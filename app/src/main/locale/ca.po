#. Data and save directories
msgctxt "Data_dir"
msgid "Data dir"
msgstr "Directori de dades"

msgctxt "Save_dir"
msgid "Save dir"
msgstr "Directori on desar"

msgctxt "Export_dir"
msgid "Export dir"
msgstr "Directori on exportar"

#. Locale selector, next to this string appears the current selected locale
msgctxt "Language"
msgid "Language"
msgstr "Llengua"

#. Enable sound and the 2 possible states it takes
msgctxt "Enable_sound"
msgid "Sound"
msgstr "So"

msgctxt "sound_enabled"
msgid "Sound enabled"
msgstr "So activat"

msgctxt "sound_disabled"
msgid "Silent"
msgstr "Silenci"

#. Enable stereo and the 2 possible states it takes
msgctxt "Enable_stereo"
msgid "Stereo sound"
msgstr "Estèreo"

msgctxt "stereo_enabled"
msgid "Stereo"
msgstr "Estèreo"

msgctxt "stereo_disabled"
msgid "Mono"
msgstr "Mono"

#. Enable autosave and the 2 possible states it takes
msgctxt "Enable_autosave"
msgid "Autosave when quiting"
msgstr "Desar automàticament en sortir"

msgctxt "autosave_enabled"
msgid "Save on quit"
msgstr "Desa en sortir"

msgctxt "autosave_disabled"
msgid "Don’t save automatically on quit"
msgstr "No desis automàticament en sortir"

#. Enable startblank and the 2 possible values it takes
msgctxt "Enable_startblank"
msgid "Start blank"
msgstr "En engegar"

msgctxt "startblanktoggle_true"
msgid "Start with a blank canvas"
msgstr "Comença amb un llenç en blanc"

msgctxt "startblanktoggle_false"
msgid "Start with last draw"
msgstr "Comença amb el darrer dibuix"

#. Enable newcolorsfirst and the 2 possible values it takes
msgctxt "Enable_newcolorsfirst"
msgid "\"New\" dialog behavior"
msgstr "Diàleg de \"Nou\" dibuix"

msgctxt "newcolorsfirsttoggle_true"
msgid "Colors on top"
msgstr "Colors a dalt"

msgctxt "newcolorsfirsttoggle_false"
msgid "Images of starters and templates on top"
msgstr "Imatges a dalt"

#. How to save: ask the user if in new draw or replacing the old
#. Allways replace the old draw          Allways save as a new draw
#. The 4 strings will display at the same time in a single row
msgctxt "Save_approach"
msgid "Save Approach"
msgstr "Desar"

msgctxt "Ask"
msgid "Ask"
msgstr "Demana sempre"

msgctxt "Replace"
msgid "Replace"
msgstr "Reemplaça l'antic"

msgctxt "New"
msgid "New"
msgstr "Desa en un dibuix nou"

#. Enable system fonts
msgctxt "Allow_sysfonts"
msgid "System fonts"
msgstr "Tipus de lletra"

msgctxt "sysfonts_enabled"
msgid "Allow system fonts"
msgstr "Tots"

msgctxt "sysfonts_disabled"
msgid "System fonts disabled"
msgstr "Només els proporcionats pel Tux Paint"

#. Printing and states
msgctxt "Allow_printing"
msgid "Printing"
msgstr "Impressió"

msgctxt "print_enabled"
msgid "Allow Printing"
msgstr "Permet"

msgctxt "print_disabled"
msgid "Printing disabled"
msgstr "Denega"

msgctxt "Print_delay"
msgid "Print delay in seconds"
msgstr "Retard entre impressions"

