msgid "Printing"
msgstr "Impression"

msgctxt "print_enabled"
msgid "Allow Printing"
msgstr "Impression activée"

msgctxt "print_disabled"
msgid "Printing disabled"
msgstr "Impression désactivée"

msgctxt "Print_delay"
msgid "Print delay in seconds"
msgstr "Délai d'impression en secondes"

#. Screensaver and states
msgctxt "Disable_screen_saver"
msgid "Screensaver"
msgstr "Économiseur d'écran"

msgctxt "screensaver_disabled"
msgid "Keep the screen on"
msgstr "Pas d'économiseur d'écran"

msgctxt "screensaver_enabled"
msgid "Allow Android blanking the screen"
msgstr "L'économiseur d'écran reste actif"

#. Orientation and states
msgctxt "Orient"
msgid "Orientation"
msgstr "Orientation"

msgctxt "orient_portrait"
msgid "Portrait"
msgstr "Portrait"

msgctxt "orient_landscape"
msgid "Landscape"
msgstr "Paysage"

#. Size of the buttons in the Tux Paint program
msgctxt "Button_size"
msgid "Button size"
msgstr "Taille des boutons"

#. Number of rows for the buttons color palette
msgctxt "Colors_rows"
msgid "Number of rows for the buttons palette"
msgstr "Nombre de colonnes pour la palette des boutons"

#. What OnScreen keyboard layout to use
msgctxt "Osk_layout"
msgid "On screen keyboard layout"
msgstr "Disposition du clavier virtuel"

#. Stamp rotation
msgctxt "Stamp_rotation"
msgid "Stamp rotation"
msgstr "Rotation des tampons"

msgctxt "Stamp_rotation_enabled"
msgid "Allow stamp rotation"
msgstr "Rotation des tampons possible"

msgctxt "Stamp_rotation_disabled"
msgid "Do not rotate stamps"
msgstr "Désactive la rotation des tampons"

#. Ok and Cancel buttons
msgctxt "ok"
msgid "Save and quit"
msgstr "Sauvegarder et quitter"

msgctxt "cancel"
msgid "Discard changes"
msgstr "Annule les changements"

#. Request for permissions text and button
msgctxt "Request_motivation"
msgid "Tux Paint may need storage access in order to save your drawings."
msgstr "Tux Paint doit pouvoir accéder au stockage pour sauvegarder vos dessins."

msgctxt "Request_btmotivation"
msgid ""
"Tux Paint may need Bluetooth access in order to operate properly with "
"some external devices. Note, to revoque BT permissions or to allow them "
"if they have been already denied you may have to do it in the Android "
"Settings."
msgstr ""
"Tux Paint doit avoir un accès au Bluetooth pour fonctionner correctement "
"avec certains dispositifs extérieurs. À noter que pour annuler les "
"permissions, ou bien pour les remettre si elles ont déjà été annulées, "
"vous devez le faire par le biais des paramètres d'Android."

msgctxt "request_understood"
msgid "Understood"
msgstr "Compris"

msgctxt "document_choose"
msgid "Choose a zip file with data for Tux Paint"
msgstr "Choisissez un fichier zip avec les données pour Tux Paint"

msgctxt "Button_select"
msgid "Select"
msgstr "Sélection"

msgctxt "textselect"
msgid "Please select a file with the data to be imported."
msgstr "Choisissez un fichier avec les données qui doivent être importées."

msgctxt "title_activity_import_data"
msgid "ImportData"
msgstr "ImportData"

