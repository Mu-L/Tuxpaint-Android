msgstr "Choisissez un fichier contenant les données à importer."

msgctxt "Files_installed"
msgid "The files have been installed correctly"
msgstr "Les fichiers ont été correctement installés"

msgctxt "Files_notinstalled"
msgid "There has been some error, the files have not been installed."
msgstr "Il y a eu une erreur, les fichiers n'ont pas été installés."

msgctxt "BTPerms"
msgid "BlueTooth"
msgstr "BlueTooth"

msgctxt "BlueTooth_allowed"
msgid "Access to BlueTooth is allowed"
msgstr "L'accès au BlueTooth est autorisé"

msgctxt "BlueTooth_denied"
