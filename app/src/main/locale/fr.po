msgctxt "next"
msgid "Next"
msgstr "Suivant"

msgctxt "previous"
msgid "Previous"
msgstr "Précédent"

msgctxt "Selected_file"
msgid "Selected file"
msgstr "Fichier sélectionné"

msgctxt "none"
msgid "None"
msgstr "Aucun"

msgctxt "destination"
msgid "Destination"
msgstr "Destination"

msgctxt "proceed"
