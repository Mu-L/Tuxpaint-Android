#. Data and save directories
msgctxt "Data_dir"
msgid "Data dir"
msgstr "Directorio de datos"

msgctxt "Save_dir"
msgid "Save dir"
msgstr "Directorio para gardar"

msgctxt "Export_dir"
msgid "Export dir"
msgstr "Directorio para exportar"

#. Locale selector, next to this string appears the current selected locale
msgctxt "Language"
msgid "Language"
msgstr "Idioma"

#. Enable sound and the 2 possible states it takes
msgctxt "Enable_sound"
msgid "Sound"
msgstr "Son"

msgctxt "sound_enabled"
msgid "Sound enabled"
msgstr "Son activado"

msgctxt "sound_disabled"
msgid "Silent"
msgstr "Silencio"

#. Enable stereo and the 2 possible states it takes
msgctxt "Enable_stereo"
msgid "Stereo sound"
msgstr "Son estéreo"

msgctxt "stereo_enabled"
msgid "Stereo"
msgstr "Estéreo"

msgctxt "stereo_disabled"
msgid "Mono"
msgstr "Mono"

#. Enable autosave and the 2 possible states it takes
msgctxt "Enable_autosave"
msgid "Autosave when quiting"
msgstr "Gardado automático cando se sae"

msgctxt "autosave_enabled"
msgid "Save on quit"
msgstr "Gardar automaticamente ao saír"

msgctxt "autosave_disabled"
msgid "Don’t save automatically on quit"
msgstr "Non gardar automaticamente ao saír"

#. Enable startblank and the 2 possible values it takes
msgctxt "Enable_startblank"
msgid "Start blank"
msgstr "Comezar en branco"

msgctxt "startblanktoggle_true"
msgid "Start with a blank canvas"
msgstr "Comezar cun lenzo en branco"

msgctxt "startblanktoggle_false"
msgid "Start with last draw"
msgstr "Comezar co último debuxo"

#. Enable newcolorsfirst and the 2 possible values it takes
msgctxt "Enable_newcolorsfirst"
msgid "\"New\" dialog behavior"
msgstr "Comportamento do diálogo «Novo»"

msgctxt "newcolorsfirsttoggle_true"
msgid "Colors on top"
msgstr "Cores enriba"

msgctxt "newcolorsfirsttoggle_false"
msgid "Images of starters and templates on top"
msgstr "Imaxes de iniciadores e modelos enriba"

#. How to save: ask the user if in new draw or replacing the old
#. Allways replace the old draw          Allways save as a new draw
#. The 4 strings will display at the same time in a single row
msgctxt "Save_approach"
msgid "Save Approach"
msgstr "Opcións de gardado"

msgctxt "Ask"
msgid "Ask"
msgstr "Preguntar sempre antes"

msgctxt "Replace"
msgid "Replace"
msgstr "Substituír o antigo"

msgctxt "New"
msgid "New"
msgstr "Gardar como debuxo novo"

#. Enable system fonts
msgctxt "Allow_sysfonts"
msgid "System fonts"
msgstr "Tipos de letra do sistema"

msgctxt "sysfonts_enabled"
msgid "Allow system fonts"
msgstr "Permitir os tipos de letra do sistema"

msgctxt "sysfonts_disabled"
msgid "System fonts disabled"
msgstr "Tipos de letra do sistema desactivados"

#. Printing and states
msgctxt "Allow_printing"
msgid "Printing"
msgstr "Impresión"

msgctxt "print_enabled"
msgid "Allow Printing"
msgstr "Permitir impresión"

msgctxt "print_disabled"
msgid "Printing disabled"
msgstr "Impresión desactivada"

msgctxt "Print_delay"
msgid "Print delay in seconds"
msgstr "Atraso da impresión en segundos"

#. Screensaver and states
msgctxt "Disable_screen_saver"
msgid "Screensaver"
msgstr "Salvapantallas"

msgctxt "screensaver_disabled"
msgid "Keep the screen on"
msgstr "Manter a pantalla activa"

msgctxt "screensaver_enabled"
msgid "Allow Android blanking the screen"
msgstr "Permitir que Android apague a pantalla"

#. Orientation and states
msgctxt "Orient"
msgid "Orientation"
msgstr "Orientacion"

msgctxt "orient_portrait"
msgid "Portrait"
msgstr "Vertical"

msgctxt "orient_landscape"
msgid "Landscape"
msgstr "Apaisado"

#. Ok and Cancel buttons
msgctxt "ok"
msgid "Save and quit"
msgstr "Gardar e saír"

msgctxt "cancel"
msgid "Discard changes"
msgstr "Desbotar os cambios"

#. Request for permissions text and button
msgctxt "Request_motivation"
msgid ""
"Tux Paint needs storage access in order to save your drawings and to "
"operate properly."
msgstr ""
"Tux Paint necesita acceso ao almacenamento para poder gardar os seus "
"debuxos e traballar correctamente."

msgctxt "request_understood"
msgid "Understood"
msgstr "Entendido"

