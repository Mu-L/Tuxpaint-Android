#. Data and save directories
msgctxt "Data_dir"
msgid "Data dir"
msgstr "Gagnamappa"

msgctxt "Save_dir"
msgid "Save dir"
msgstr "Mappa til að vista"

msgctxt "Export_dir"
msgid "Export dir"
msgstr "Útflutningsmappa"

#. Locale selector, next to this string appears the current selected locale
msgctxt "Language"
msgid "Language"
msgstr "Tungumál"

#. Enable sound and the 2 possible states it takes
msgctxt "Enable_sound"
msgid "Sound"
msgstr "Hljóð"

msgctxt "sound_enabled"
msgid "Sound enabled"
msgstr "Hljóð virkt"

msgctxt "sound_disabled"
msgid "Silent"
msgstr "Þögn"

#. Enable stereo and the 2 possible states it takes
msgctxt "Enable_stereo"
msgid "Stereo sound"
msgstr "Víðóma hljóð"

msgctxt "stereo_enabled"
msgid "Stereo"
msgstr "Víðóma"

msgctxt "stereo_disabled"
msgid "Mono"
msgstr "Einóma"

#. Enable autosave and the 2 possible states it takes
msgctxt "Enable_autosave"
msgid "Autosave when quiting"
msgstr "Vista sjálfkrafa við að hætta"

msgctxt "autosave_enabled"
msgid "Save on quit"
msgstr "Vista við að hætta"

msgctxt "autosave_disabled"
msgid "Don’t save automatically on quit"
msgstr "Ekki vista sjálfkrafa við að hætta"

#. Enable startblank and the 2 possible values it takes
msgctxt "Enable_startblank"
msgid "Start blank"
msgstr "Byrja autt"

msgctxt "startblanktoggle_true"
msgid "Start with a blank canvas"
msgstr "Byrja með auðan myndflöt"

msgctxt "startblanktoggle_false"
msgid "Start with last draw"
msgstr "Byrja með síðustu teikningu"

#. Enable newcolorsfirst and the 2 possible values it takes
msgctxt "Enable_newcolorsfirst"
msgid "\"New\" dialog behavior"
msgstr "Hegðun \"Nýtt\"-gluggans"

msgctxt "newcolorsfirsttoggle_true"
msgid "Colors on top"
msgstr "Litir efst"

msgctxt "newcolorsfirsttoggle_false"
msgid "Images of starters and templates on top"
msgstr "Myndir af mögulegum byrjunum og sniðmátum efst"

#. How to save: ask the user if in new draw or replacing the old
#. Allways replace the old draw          Allways save as a new draw
#. The 4 strings will display at the same time in a single row
msgctxt "Save_approach"
msgid "Save Approach"
msgstr "Hegðun við vistun"

msgctxt "Ask"
msgid "Ask"
msgstr "Spyrja"

msgctxt "Replace"
msgid "Replace"
msgstr "Skipta út"

msgctxt "New"
msgid "New"
msgstr "Ný teikning"

#. Enable system fonts
msgctxt "Allow_sysfonts"
msgid "System fonts"
msgstr "Kerfisletur"

msgctxt "sysfonts_enabled"
msgid "Allow system fonts"
msgstr "Leyfa kerfisletur"

msgctxt "sysfonts_disabled"
msgid "System fonts disabled"
msgstr "Kerfisletur óvirkt"

#. Printing and states
msgctxt "Allow_printing"
msgid "Printing"
msgstr "Prentun"

msgctxt "print_enabled"
msgid "Allow Printing"
msgstr "Leyfa prentun"

msgctxt "print_disabled"
msgid "Printing disabled"
msgstr "Prentun óvirk"

msgctxt "Print_delay"
msgid "Print delay in seconds"
msgstr "Töf við prentun í sekúndum"

#. Screensaver and states
msgctxt "Disable_screen_saver"
msgid "Screensaver"
msgstr "Skjáhvíla"

msgctxt "screensaver_disabled"
msgid "Keep the screen on"
msgstr "Halda kveiktu á skjánum"

msgctxt "screensaver_enabled"
msgid "Allow Android blanking the screen"
msgstr "Leyfa Android að tæma skjáinn"

#. Orientation and states
msgctxt "Orient"
msgid "Orientation"
msgstr "Stefna"

msgctxt "orient_portrait"
msgid "Portrait"
msgstr "Lóðrétt"

msgctxt "orient_landscape"
msgid "Landscape"
msgstr "Lárétt"

#. Size of the buttons in the Tux Paint program
msgctxt "Button_size"
msgid "Button size"
msgstr "Stærð hnapps"

#. Number of rows for the buttons color palette
msgctxt "Colors_rows"
msgid "Number of rows for the buttons palette"
msgstr "Fjöldi raða í hnappaspjaldinu"

#. What OnScreen keyboard layout to use
msgctxt "Osk_layout"
msgid "On screen keyboard layout"
msgstr "Framsetning skjályklaborðs"

#. Stamp rotation
msgctxt "Stamp_rotation"
msgid "Stamp rotation"
msgstr "Snúningur stimpla"

msgctxt "Stamp_rotation_enabled"
msgid "Allow stamp rotation"
msgstr "Leyfa snúning stimpla"

msgctxt "Stamp_rotation_disabled"
msgid "Do not rotate stamps"
msgstr "Ekki snúna stimplum"

#. Ok and Cancel buttons
msgctxt "ok"
msgid "Save and quit"
msgstr "Vista og hætta"

msgctxt "cancel"
msgid "Discard changes"
msgstr "Henda breytingum"

#. Request for permissions text and button
msgctxt "Request_motivation"
msgid "Tux Paint may need storage access in order to save your drawings."
msgstr ""
"Tux Paint gæti þarfnast aðgangs að geymslrými til að geta vistað"
" teikningarnar þínar."

msgctxt "Request_btmotivation"
msgid ""
"Tux Paint may need Bluetooth access in order to operate properly with "
"some external devices. Note, to revoque BT permissions or to allow them "
"if they have been already denied you may have to do it in the Android "
"Settings."
msgstr ""
"Tux Paint gæti þurft aðgang að Bluetooth til að virka rétt með sumum"
" utanaðkomandi tækjum. Athugaðu að til að afturkalla Bluetooth-heimildir eða"
" veita þær ef ekki var búið að gefa heimildir, gætirðu þurft að gera það í"
" stillingum Android-kerfisins á tækinu."

msgctxt "request_understood"
msgid "Understood"
msgstr "Skil þetta"

msgctxt "document_choose"
msgid "Choose a zip file with data for Tux Paint"
msgstr "Veldu ZIP-skrá með gögnum fyrir Tux Paint"

msgctxt "Button_select"
msgid "Select"
msgstr "Veldu"

msgctxt "textselect"
msgid "Please select a file with the data to be imported."
msgstr "Veldu skrá með gögnunum sem á að flytja inn."

msgctxt "title_activity_import_data"
msgid "ImportData"
msgstr "Flytja inn gögn"

#. Strings used for fragments for navigation
msgctxt "next"
msgid "Next"
msgstr "Næsta"

msgctxt "previous"
msgid "Previous"
msgstr "Fyrra"

msgctxt "Selected_file"
msgid "Selected file"
msgstr "Valin skrá"

msgctxt "none"
msgid "None"
msgstr "Engin"

msgctxt "destination"
msgid "Destination"
msgstr "Áfangastaður"

msgctxt "proceed"
msgid "Proceed"
msgstr "Halda áfram"

msgctxt "back_from_import"
msgid "Back"
msgstr "Til baka"

msgctxt "please_select"
msgid "Please select a Zip file with data to be imported."
msgstr "Veldu ZIP-skrá með gögnum sem á að flytja inn."

msgctxt "Files_installed"
msgid "The files have been installed correctly"
msgstr "Það tókst að setja upp skrárnar"

msgctxt "Files_notinstalled"
msgid "There has been some error, the files have not been installed."
msgstr "Einhver villa hefur komið upp, skrárnar hafa ekki verið settar upp."

msgctxt "BTPerms"
msgid "BlueTooth"
msgstr "Bluetooth"

msgctxt "BlueTooth_allowed"
msgid "Access to BlueTooth is allowed"
msgstr "Aðgangur að Bluetooth er leyfður"

msgctxt "BlueTooth_denied"
msgid "Tux Paint has not access to BlueTooth"
msgstr "Tux Paint er ekki með aðgang að Bluetooth"

msgctxt "requestrevoquebt"
msgid "BlueTooth info"
msgstr "Bluetooth-upplýsingar"
