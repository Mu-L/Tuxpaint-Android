msgstr "`\\%_@$~#{<(^&*"

#. digits (e.g., '0', '1' and '7')
#: ../dirwalk.c:250
msgid "017"
msgstr "017"

#. distinct circle-like characters (e.g., 'O' (capital oh) vs. '0' (zero))
#: ../dirwalk.c:255
msgid "O0"
msgstr "O0"

#. distinct line-like characters (e.g., 'l' (lowercase elle) vs. '1' (one) vs. 'I' (capital aye))
